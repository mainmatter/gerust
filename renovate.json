{
  "extends": ["github>mainmatter/renovate-config:default.json5"],
  {
    "customManagers": [
      {
        "customType": "regex",
        "fileMatch": ["^Cargo.toml.liquid$"],
        "matchStrings": ["^(?!name|version|edition|path)(?<depName>[a-zA-Z_]+)\s*=\s*({.*version\s*=\s*)?\"(?<currentValue>.*?)\""],
        "datasourceTemplate": "crate"
      }
    ]
  }
}
